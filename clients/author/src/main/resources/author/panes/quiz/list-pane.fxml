<?xml version="1.0"?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.*?>
<?import javafx.geometry.*?>
<TableView fx:id="list" VBox.vgrow="ALWAYS" xmlns:fx="http:/javafx.com/fxml">
  <columns>
    <TableColumn fx:id="id" text="ID" sortable="true" prefWidth="${list.width * 0.2}" />
    <TableColumn fx:id="title" text="Title" sortable="true"
    prefWidth="${list.width * 0.6}" />
    <TableColumn fx:id="status" text="Status" sortable="true"
    prefWidth="${list.width * 0.15}" />
    <TableColumn fx:id="obsolete" text="Obs" sortable="true"
    prefWidth="${list.width * 0.05}" />
  </columns>
</TableView>
